# Adobe PDF Extract API
ADOBE_CLIENT_ID=your_adobe_client_id_here
ADOBE_CLIENT_SECRET=your_adobe_client_secret_here

# AWS Textract
AWS_ACCESS_KEY_ID=your_aws_access_key_here
AWS_SECRET_ACCESS_KEY=your_aws_secret_key_here
AWS_DEFAULT_REGION=us-east-1

# Google Document AI
GOOGLE_APPLICATION_CREDENTIALS=/path/to/your/service-account-key.json
GCP_PROJECT_ID=your_gcp_project_id
GCP_PROCESSOR_ID=your_processor_id
GCP_LOCATION=us

# Azure Document Intelligence
AZURE_DOCUMENT_INTELLIGENCE_ENDPOINT=https://your-resource.cognitiveservices.azure.com/
AZURE_DOCUMENT_INTELLIGENCE_KEY=your_azure_key_here

# Tesseract OCR (if not in PATH)
TESSERACT_CMD=/usr/local/bin/tesseract

# Poppler (for pdf2image, if not in PATH)
POPPLER_PATH=/usr/local/bin
